@model List<MVCProject.Common.ViewModels.ZuuCargoVM>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1, minimal-ui">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="apple-mobile-web-app-status-bar-style" content="black">
    <link rel="apple-touch-icon" href="images/apple-touch-icon.png" />
    <link rel="apple-touch-startup-image" href="images/apple-touch-startup-image-640x920.png">
    <title>mobili - mobile template</title>
    <link rel="stylesheet" href="css/swiper.css">
    <link rel="stylesheet" href="style.css">
    <link href="https://fonts.googleapis.com/css?family=Roboto:100,300,400,900" rel="stylesheet">
</head>
<body id="mobile_wrap">

    <div class="panel-overlay"></div>

    <div class="panel panel-left panel-reveal">
        <!-- Slider -->
        <div class="swiper-container-subnav multinav">
            <div class="swiper-wrapper">
                <div class="swiper-slide">
                    <nav class="main_nav_underline">
                        <ul>
                            <li><a href="index.html"><img src="images/icons/gray/home.png" alt="" title="" /><span>Home</span></a></li>
                            <li><a href="about.html"><img src="images/icons/gray/mobile.png" alt="" title="" /><span>About</span></a></li>
                            <li><a href="features.html"><img src="images/icons/gray/features.png" alt="" title="" /><span>Features</span></a></li>

                            <li><a href="#" data-popup=".popup-login" class="open-popup"><img src="images/icons/gray/lock.png" alt="" title="" /><span>Login</span></a></li>
                            <li><a href="team.html"><img src="images/icons/gray/users.png" alt="" title="" /><span>Team</span></a></li>
                            <li><a href="blog.html"><img src="images/icons/gray/blog.png" alt="" title="" /><span>Blog</span></a></li>

                            <li><a href="photos.html"><img src="images/icons/gray/photos.png" alt="" title="" /><span>Photos</span></a></li>
                            <li><a href="videos.html"><img src="images/icons/gray/video.png" alt="" title="" /><span>Videos</span></a></li>
                            <li><a href="music.html"><img src="images/icons/gray/music.png" alt="" title="" /><span>Music</span></a></li>

                            <li><a href="shop.html"><img src="images/icons/gray/shop.png" alt="" title="" /><span>Shop</span></a></li>
                            <li class="subnav opensubnav"><img src="images/icons/gray/categories.png" alt="" title="" /><span>Sub level menu</span></li>
                            <li><a href="cart.html"><img src="images/icons/gray/cart.png" alt="" title="" /><span>Cart</span></a></li>

                            <li><a href="tables.html"><img src="images/icons/gray/tables.png" alt="" title="" /><span>Tables</span></a></li>

                            <li><a href="form.html"><img src="images/icons/gray/form.png" alt="" title="" /><span>Custom Form</span></a></li>


                            <li><a href="tel:012345678"><img src="images/icons/gray/phone.png" alt="" title="" /><span>Call now!</span></a></li>
                            <li><a href="contact.html"><img src="images/icons/gray/contact.png" alt="" title="" /><span>Contact</span></a></li>
                        </ul>
                    </nav>
                </div>
                <div class="swiper-slide">
                    <div class="subnav_header backtonav"><img src="images/icons/gray/back.png" alt="" title="" /><span>BACK TO MAIN MENU</span></div>
                    <nav class="main_nav_underline">
                        <ul>

                            <li><a href="shop.html"><img src="images/icons/gray/car.png" alt="" title="" /><span>Cars</span></a></li>
                            <li><a href="shop.html"><img src="images/icons/gray/bus.png" alt="" title="" /><span>Buses</span></a></li>
                            <li><a href="shop.html"><img src="images/icons/gray/bike.png" alt="" title="" /><span>Bikes</span></a></li>

                            <li><a href="shop.html"><img src="images/icons/gray/drink.png" alt="" title="" /><span>Drinks</span></a></li>
                            <li><a href="shop.html"><img src="images/icons/gray/food.png" alt="" title="" /><span>Food</span></a></li>
                            <li><a href="shop.html"><img src="images/icons/gray/orders.png" alt="" title="" /><span>Clothes</span></a></li>

                            <li><a href="shop.html"><img src="images/icons/gray/rocket.png" alt="" title="" /><span>Rockets</span></a></li>
                            <li><a href="shop.html"><img src="images/icons/gray/briefcase.png" alt="" title="" /><span>Accessories</span></a></li>
                            <li><a href="shop.html"><img src="images/icons/gray/gift.png" alt="" title="" /><span>Gifts</span></a></li>
                            <li class="subnav opensubsubnav"><img src="images/icons/gray/categories.png" alt="" title="" /><span>Third sublevel menu</span></li>
                        </ul>
                    </nav>
                </div>
                <div class="swiper-slide">
                    <div class="subnav_header backtosubnav"><img src="images/icons/gray/back.png" alt="" title="" /><span>BACK TO 2 SUBLEVEL MENU</span></div>
                    <nav class="main_nav_underline">
                        <ul>

                            <li><a href="shop.html"><img src="images/icons/gray/car.png" alt="" title="" /><span>Subcategory 01</span></a></li>
                            <li><a href="shop.html"><img src="images/icons/gray/bus.png" alt="" title="" /><span>Subcategory 02</span></a></li>
                            <li><a href="shop.html"><img src="images/icons/gray/bike.png" alt="" title="" /><span>Subcategory 03</span></a></li>
                            <li><a href="shop.html"><img src="images/icons/gray/drink.png" alt="" title="" /><span>Subcategory 04</span></a></li>
                            <li><a href="shop.html"><img src="images/icons/gray/food.png" alt="" title="" /><span>Subcategory 05</span></a></li>


                        </ul>
                    </nav>
                </div>
            </div>
        </div>
    </div>

    <div class="panel panel-right panel-reveal">
        <div class="user_login_info">

            <div class="user_thumb">

                <div class="user_details">
                    <p>Welcome, <span>Nathalie</span></p>
                </div>
                <div class="user_avatar"><img src="images/avatar3.jpg" alt="" title="" /></div>
            </div>

            <nav class="user-nav">
                <ul>
                    <li><a href="features.html"><img src="images/icons/gray/settings.png" alt="" title="" /><span>Account Settings</span></a></li>
                    <li><a href="features.html"><img src="images/icons/gray/briefcase.png" alt="" title="" /><span>My Account</span></a></li>
                    <li><a href="features.html"><img src="images/icons/gray/message.png" alt="" title="" /><span>Messages</span><strong>12</strong></a></li>
                    <li><a href="features.html"><img src="images/icons/gray/love.png" alt="" title="" /><span>Favorites</span><strong>5</strong></a></li>
                    <li><a href="index.html"><img src="images/icons/gray/lock.png" alt="" title="" /><span>Logout</span></a></li>
                </ul>
            </nav>
        </div>
    </div>

    <div class="views">

        <div class="view view-main">
            <div class="pages">
                <div data-page="tables" class="page">
                    <div class="page-content">

                        <div class="navbar navbar--fixed navbar--fixed-top navbar--bg">
                            <div class="navbar__col navbar__col--title">
                                <a href="http://ZuuCargo.com">ZuuCargo</a>
                            </div>
                            <div class="navbar__col navbar__col--icon navbar__col--icon-right">
                                <a href="cart.html"><img src="images/icons/white/cart.png" alt="" title="" /><span class="navbar__icon-bullet">3</span></a>
                            </div>
                            <div class="navbar__col navbar__col--icon navbar__col--icon-right">
                                <a href="#" data-panel="right" class="open-panel"><img src="images/icons/white/user.png" alt="" title="" /></a>
                            </div>
                            <div class="navbar__col navbar__col--icon navbar__col--icon-right">
                                <a href="#" data-panel="left" class="open-panel"><img src="images/icons/white/menu.png" alt="" title="" /></a>
                            </div>
                        </div>

                        <div id="pages_maincontent">



                            <div class="page_single layout_fullwidth_padding">

                                <h3>Cargo List</h3> <h3>Total :  @Model.Select(i => Convert.ToDouble(i.Price)).Sum() $</h3>
                                @foreach (MVCProject.Common.ViewModels.ZuuCargoVM kargoVM in Model)
                                {
                                    <ul class="responsive_table">
                                        <li class="table_row">

                                            <div class="table_section">Stock Name</div>
                                            <div class="table_section">@kargoVM.StockName</div>
                                        </li>
                                        <li class="table_row">

                                            <div class="table_section">Number Of Container</div>
                                            <div class="table_section">@kargoVM.NumberOfContainer</div>
                                        </li>
                                        <li class="table_row">

                                            <div class="table_section">Kg</div>
                                            <div class="table_section">@kargoVM.Kg</div>
                                        </li>
                                        <li class="table_row">

                                            <div class="table_section">Pcs</div>
                                            <div class="table_section">@kargoVM.Pcs</div>
                                        </li>
                                        <li class="table_row">

                                            <div class="table_section">Price</div>
                                            <div class="table_section">@kargoVM.Price</div>
                                        </li>
                                        <li class="table_row">

                                            <div class="table_section">Date</div>
                                            <div class="table_section">@kargoVM.Date</div>
                                        </li>
                                        <li class="table_row">

                                            <div class="table_section">Notes</div>
                                            <div class="table_section">@kargoVM.Notes</div>
                                        </li>
                                    </ul>

                                }

                                @*<p>With this table design you can create a list of web design services.</p>*@



                            </div>

                        </div>


                    </div>
                </div>
            </div>
        </div>
    </div>





    <script src="js/jquery-3.3.1.min.js"></script>
    <script src="js/jquery.validate.min.js"></script>
    <script src="js/swiper.min.js"></script>

    <script src="js/jquery.custom.js"></script>
</body>
</html>