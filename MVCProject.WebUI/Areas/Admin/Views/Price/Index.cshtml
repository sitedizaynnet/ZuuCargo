@model IEnumerable<MVCProject.Common.ViewModels.PriceVM>

@{
    ViewBag.Title = "Prices";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutAdmin.cshtml";
}

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>

        <th>
            @Html.DisplayNameFor(model => model.Type)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.FromDesi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ToDesi)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Zone)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsExpress)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ServicePrice)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>

    <td>
        @Html.DisplayFor(modelItem => item.Type)
    </td>

    <td>
        @Html.DisplayFor(modelItem => item.FromDesi)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.ToDesi)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.Zone)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.IsExpress)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.ServicePrice)
    </td>
    <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
        @Html.ActionLink("Details", "Details", new { id = item.Id }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.Id })
    </td>
</tr>
}

</table>
