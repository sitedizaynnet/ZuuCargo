<script src="~/Theme/Admin/plugins/slimScroll/jquery.slimscroll.min.js"></script>
<!-- FastClick -->
<script src="~/Theme/Admin/plugins/fastclick/fastclick.js"></script>
<!-- AdminLTE App -->
<script src="~/Theme/Admin/dist/js/app.min.js"></script>
<!-- AdminLTE dashboard demo (This is only for demo purposes) -->
<script src="~/Theme/Admin/dist/js/pages/dashboard.js"></script>
<!-- AdminLTE for demo purposes -->
<script src="~/Theme/Admin/dist/js/demo.js"></script>


</body>
</html>
